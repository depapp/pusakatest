    "repository": {
      "type": "git",
      "url": "git+https://github.com/depapp/pusakatest.git"
    },
    "author": "@depapp",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/depapp/pusakatest/issues"
    },
    "homepage": "https://github.com/depapp/pusakatest#readme",
    "devDependencies": {
      "@cucumber/cucumber": "latest",
      "@faker-js/faker": "latest",
      "mocha": "latest",
      "pactum": "latest"
    }
  }
  